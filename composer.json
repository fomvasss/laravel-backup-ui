{
    "name": "fomvasss/laravel-backup-ui",
    "description": "Web interface for spatie/laravel-backup package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fomvasss",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-backup": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\LaravelBackupUi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\LaravelBackupUi\\BackupUiServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html build/coverage",
        "test-unit": "vendor/bin/phpunit --testsuite=Unit",
        "test-feature": "vendor/bin/phpunit --testsuite=Feature"
    }
}
